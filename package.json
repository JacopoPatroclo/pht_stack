{
  "name": "fastify-htmx",
  "version": "1.0.0",
  "description": "A Fastify application with HTMX and TailwindCSS",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "node build.mjs",
    "dev": "node build.mjs --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "xss-scan && node build.mjs --test && node --test dist",
    "typecheck": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.client.json --noEmit",
    "check": "pnpm typecheck && pnpm test && pnpm audit"
  },
  "keywords": [
    "htmx",
    "tailwindcss",
    "typescript",
    "drizzle-orm",
    "postgres",
    "html",
    "kitajs",
    "esbuild",
    "htmx.org",
    "pg",
    "env-schema",
    "fastify"
  ],
  "author": "Jacopo Martinelli",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "5.8.0",
    "@fastify/error": "3.4.1",
    "@fastify/sensible": "5.5.0",
    "@fastify/static": "7.0.1",
    "@fastify/type-provider-typebox": "4.0.0",
    "@kitajs/fastify-html-plugin": "1.0.0",
    "@kitajs/html": "3.1.2",
    "@sinclair/typebox": "0.32.14",
    "close-with-grace": "1.2.0",
    "drizzle-orm": "0.29.3",
    "env-schema": "5.2.1",
    "fastify": "4.26.1",
    "fastify-plugin": "4.5.1",
    "postgres": "3.4.3"
  },
  "devDependencies": {
    "@kitajs/ts-html-plugin": "1.3.4",
    "@types/node": "20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "drizzle-kit": "0.20.14",
    "esbuild": "0.20.0",
    "eslint": "^8.56.0",
    "htmx.org": "1.9.10",
    "pg": "8.11.3",
    "pino-pretty": "10.3.1",
    "prettier": "3.2.5",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  }
}
