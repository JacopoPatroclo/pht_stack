{
  "name": "fastify-htmx",
  "version": "1.0.0",
  "description": "A Fastify application with HTMX and TailwindCSS",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "sp",
    "dev": "sp --dev",
    "test": "sp --test",
    "e2e": "spe",
    "lint": "xss-scan && eslint . --ext .ts,.tsx",
    "typecheck": "tsc -p tsconfig.app.json --noEmit & tsc -p tsconfig.client.json --noEmit",
    "gen-session-key": "spk .env.local"
  },
  "keywords": [
    "htmx",
    "tailwindcss",
    "typescript",
    "drizzle-orm",
    "postgres",
    "html",
    "kitajs",
    "esbuild",
    "htmx.org",
    "pg",
    "env-schema",
    "fastify"
  ],
  "author": "Jacopo Martinelli",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "5.8.0",
    "@fastify/error": "3.4.1",
    "@fastify/formbody": "7.4.0",
    "@fastify/multipart": "8.1.0",
    "@fastify/secure-session": "7.1.0",
    "@fastify/sensible": "5.5.0",
    "@fastify/static": "7.0.1",
    "@fastify/type-provider-typebox": "4.0.0",
    "@kitajs/fastify-html-plugin": "1.0.0",
    "@kitajs/html": "3.1.2",
    "@sinclair/typebox": "0.32.15",
    "close-with-grace": "1.3.0",
    "drizzle-orm": "0.30.1",
    "env-schema": "5.2.1",
    "fastify": "4.26.2",
    "fastify-better-flash": "0.0.2",
    "fastify-plugin": "4.5.1",
    "postgres": "3.4.3"
  },
  "devDependencies": {
    "@kitajs/ts-html-plugin": "1.3.4",
    "@types/node": "20.11.27",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "cypress": "13.6.6",
    "drizzle-kit": "0.20.14",
    "esbuild": "0.20.1",
    "eslint": "8.57.0",
    "eslint-plugin-drizzle": "0.2.3",
    "htmx.org": "1.9.10",
    "pg": "8.11.3",
    "pino-pretty": "10.3.1",
    "prettier": "3.2.5",
    "slim-pack": "0.0.3",
    "tailwindcss": "3.4.1",
    "typescript": "5.4.2"
  }
}
